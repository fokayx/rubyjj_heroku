<!DOCTYPE html>
<html>
<head>
  <title>Humandock</title>
  <%= stylesheet_link_tag    'admin_application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'admin_application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag  %>
</head>
<body>
  <%= render 'application/admin_navbar' %>
  <div class="main-container" id="main-container">
    <%= render 'application/admin_sidebar' %>
    <div class="main-content">
      <%= render 'application/admin_breadcrumb' %>
      <div class="page-content">
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>
